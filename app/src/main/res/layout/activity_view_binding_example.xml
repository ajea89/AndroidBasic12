<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ejerciciosclase.ejercicio2.componentesgraficos.viewbinding.ViewBindingExampleActivity">

    <TextView
        android:id="@+id/tvName"
        android:text="Texto 1"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/tvSurname"
        android:text="Texto 2"
        app:layout_constraintTop_toBottomOf="@+id/tvName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/tvLastname"
        android:text="Texto 3"
        app:layout_constraintTop_toBottomOf="@+id/tvSurname"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/tvFinal"
        android:text="Texto 4"
        app:layout_constraintTop_toBottomOf="@+id/tvLastname"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

</androidx.constraintlayout.widget.ConstraintLayout>